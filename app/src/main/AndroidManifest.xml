<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Firebase"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SalesActivity"
            android:exported="true"
            android:label="판매 글 목록"
            ></activity>
        <activity android:name=".SignUpActivity"
            android:exported="true"
            android:label="회원가입"
            ></activity>
        <activity android:name=".PostActivity"
            android:exported="true"
            android:label="판매 글 작성"
            ></activity>
        <activity android:name=".DetailActivity"
            android:exported="true"
            android:label="상세글"
            ></activity>
        <activity android:name=".EditActivity"
            android:exported="true"
            android:label="글수정"
            ></activity>
        <activity android:name=".SendMsgActivity"
            android:exported="true"
            android:label="메시지보내기"
            ></activity>
        <activity android:name=".ReceiveMsgActivity"
            android:exported="true"
            android:label="메시지확인하기"
            ></activity>
        <activity android:name=".DetailMessageActivity"
            android:exported="true"
            android:label="메시지상세글"
            ></activity>
        <activity android:name=".ProfileActivity"
            android:exported="true"
            android:label="프로필"
            ></activity>
    </application>

</manifest>